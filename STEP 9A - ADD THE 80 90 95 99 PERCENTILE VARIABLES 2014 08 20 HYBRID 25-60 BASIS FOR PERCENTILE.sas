%MEND FNAMES;
%PUT %FNAMES;


DATA HHHEADSELECTEDVARSYBPTWA_ALL;
MERGE PSID.HOUSEHOLDHEADSELECTEDVARSYBPTW %FNAMES;
BY ER30001 ER30002;
