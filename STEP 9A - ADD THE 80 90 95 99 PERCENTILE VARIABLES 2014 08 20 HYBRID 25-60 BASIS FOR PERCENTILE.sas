%MACRO CUTPOINTS;
%DO I = 1968 %TO 1997;

*create the hybrid_calculatedage variable for each wave;
DATA HHHEADSELECTEDVARSYBPTWH_&I;
SET PSID.HOUSEHOLDHEADSELECTEDVARSYBPTW (KEEP = ER30001 ER30002 HYBRID_INDIVYEAROFBIRTH TOTFAMINC&I);
HYBRID_CALCULATEDAGE&I = &I - HYBRID_INDIVYEAROFBIRTH; 
RUN;

*create the percentiles;
PROC MEANS DATA = HHHEADSELECTEDVARSYBPTWH_&I P1 P5 P10 P20 P80 P90 P95 P99;
VAR TOTFAMINC&I ;
WHERE ER30001 < 7000 AND HYBRID_CALCULATEDAGE&I GE 25 AND HYBRID_CALCULATEDAGE&I LE 60;
ODS OUTPUT SUMMARY = INCOMEPCTL15102080909599_&I ;
RUN;

*combine the hybrid_calculatedage and percentile files;
PROC SQL;
CREATE TABLE HHHEADSELECTEDVARSYBPTWA_&I AS
SELECT ER30001, ER30002, HYBRID_CALCULATEDAGE&I, TOTFAMINC&I._P1, TOTFAMINC&I._P5, TOTFAMINC&I._P10, TOTFAMINC&I._P20, TOTFAMINC&I._P80, TOTFAMINC&I._P90, TOTFAMINC&I._P95, TOTFAMINC&I._P99
FROM HHHEADSELECTEDVARSYBPTWH_&I, (SELECT * FROM INCOMEPCTL15102080909599_&I)
ORDER BY ER30001, ER30002;
QUIT;

%END;

%DO I = 1999 %TO 2011 %BY 2;

*create the hybrid_calculatedage variable for each wave;
DATA HHHEADSELECTEDVARSYBPTWH_&I;
SET PSID.HOUSEHOLDHEADSELECTEDVARSYBPTW (KEEP = ER30001 ER30002 HYBRID_INDIVYEAROFBIRTH TOTFAMINC&I);
HYBRID_CALCULATEDAGE&I = &I - HYBRID_INDIVYEAROFBIRTH; 
RUN;

*create the percentiles;
PROC MEANS DATA = HHHEADSELECTEDVARSYBPTWH_&I P1 P5 P10 P20 P80 P90 P95 P99;
VAR TOTFAMINC&I ;
WHERE ER30001 < 7000 AND HYBRID_CALCULATEDAGE&I GE 25 AND HYBRID_CALCULATEDAGE&I LE 60;
ODS OUTPUT SUMMARY = INCOMEPCTL15102080909599_&I ;
RUN;

*combine the hybrid_calculatedage and percentile files;
PROC SQL;
CREATE TABLE HHHEADSELECTEDVARSYBPTWA_&I AS
SELECT ER30001, ER30002, HYBRID_CALCULATEDAGE&I, TOTFAMINC&I._P1, TOTFAMINC&I._P5, TOTFAMINC&I._P10, TOTFAMINC&I._P20, TOTFAMINC&I._P80, TOTFAMINC&I._P90, TOTFAMINC&I._P95, TOTFAMINC&I._P99
FROM HHHEADSELECTEDVARSYBPTWH_&I, (SELECT * FROM INCOMEPCTL15102080909599_&I)
ORDER BY ER30001, ER30002;
QUIT;

%END;


%MEND CUTPOINTS;
%CUTPOINTS;

*list the names of the temp files that will be merged in the data step below;
%MACRO FNAMES;
%DO I = 1968 %TO 1997;
HHHEADSELECTEDVARSYBPTWA_&I
%END;
%DO I = 1999 %TO 2011 %BY 2;
HHHEADSELECTEDVARSYBPTWA_&I
